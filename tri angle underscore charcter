// Online C compiler to run C program online
#include <stdio.h>
void main(){
    int n, row, col;
    printf("Enter your desire number:\n");
    scanf("%d", &n);
    
    for(row=1; row<=n; row++){
        for(col=1; col<=row; col++){
            printf("%c ", col+96); //for character underscore it using 96 and %c//
        }
        printf("\n");
    }
    
    
    
}
