#include <stdio.h>
int main(){
    int i, num, b[100];
    printf("Input your number for store in the array: \n");
    scanf("%d", &num);
    
    for(i=0; i<num; i++){
        printf("Number- %d: ", i);
        scanf("%d", &b[i]);
    }
    printf("Reverse order to your given input: \n");
    
    for(i=num-1; i>=0; i--){
        printf("%  d", b[i]);
    }
    return 0;
}
