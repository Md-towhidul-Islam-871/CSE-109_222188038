// Online C compiler to run C program online
#include <stdio.h>
void main(){
    int a, sum=0, avg;
    printf("1st 10 numbers are: ");
    for(a=1; a<=10 ; a++){
        sum+=a;
        avg=sum/10;
        printf("%d ", a);
    }
    printf("\nThe sum of 1st 10 numbers: %d", sum);
    printf("\nThe avarage of the sum is: %d", avg);

}
