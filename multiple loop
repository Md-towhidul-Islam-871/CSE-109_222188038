// Online C compiler to run C program online
#include <stdio.h>

void main(){
    int a, b, c;
    
    printf("Enter 1st Number:\n");
    scanf("%d", &a);
    
    printf("Enter 2nd Number:\n");
    scanf("%d", &b);
    
    printf("Enter 3rd Number:\n");
    scanf("%d", &c);
    
    for(a=0, b=0, c=0; a<11, b<22, c<33 ;a++, b+=2, c+=3){
        printf("%d %d %d\n", a,b,c);
    }
    
}
